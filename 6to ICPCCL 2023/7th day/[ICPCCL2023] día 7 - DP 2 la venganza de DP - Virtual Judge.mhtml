#solutionModal #info-panel td { background-color: rgb(255, 255, 255); }

.comment-posts-container { font-size: 14px; margin-left: 15px; }

li.comment-post { clear: both; list-style: none; margin-top: 5px; }

.comment-posts-container .author-avatar img { border-radius: 20%; max-heigh=
t: 75px; width: 100%; }

li.comment-post > table .content { -webkit-font-smoothing: antialiased; bac=
kground-color: rgba(255, 255, 255, 0.8); border-radius: 10px; overflow: hid=
den; padding: 10px 20px; }

li.comment-post.spam > table .content { background-color: rgba(255, 0, 0, 0=
.05); }

