li.comment-post > table .content p { margin-bottom: 0px; margin-top: 0px; }

li.comment-post > table .content img { max-width: 90%; }

li.comment-post > table .operation { display: none; }

li.comment-post:hover > table .operation { display: inline; }

li.comment-post summary { display: list-item; }

div.comment-edit button { float: right; }

div.comment-edit buttons { padding-bottom: 5px; }

div.comment-edit .preview { -webkit-font-smoothing: antialiased; background=
-color: rgba(255, 255, 255, 0.8); border-radius: 10px; padding: 10px 20px; =
}

div.comment-edit .preview span { color: grey; }

div.show-spam { -webkit-font-smoothing: antialiased; background-color: rgba=
(199, 199, 199, 0.8); border-radius: 10px; padding: 10px 20px; text-align: =
center; }

#prob-title h2 { display: inline; }

#prob-title .origin { padding-left: 20px; }

#prob-operation .row > div { padding: 3px; }

#prob-operation a, #prob-operation button { padding-left: 4px; padding-righ=
t: 4px; width: 100%; }

#prob-title { padding-left: 55px; }

#prob-left-panel, #prob-right-panel { padding-top: 30px; }

#prob-alert { margin-left: -15px; margin-right: -15px; }

#prob-properties { margin-top: 10px; }

#prob-properties dl, #prob-statistics dl { font-size: 12px; overflow: hidde=
n; padding: 5px 0px; }

#prob-properties dd, #prob-properties dt, #prob-statistics dd, #prob-statis=
tics dt { margin-bottom: 0px; padding: 5px 5px 5px 15px; }

#prob-my-sols { font-size: 12px; margin-left: -15px; margin-right: -15px; }

#prob-my-sols table { table-layout: fixed; }

#prob-my-sols th.result { width: 60%; }

#prob-my-sols td { background-color: rgb(255, 255, 255); cursor: pointer; p=
adding: 5px; text-align: center; }

#prob-my-sols td.accepted { color: rgb(92, 184, 92); font-weight: 700; }

#prob-my-sols td.failed { color: rgb(217, 83, 79); }

#prob-my-sols td.pending { color: grey; }

#prob-descriptions { max-height: 500px; overflow: auto; }

